{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Fastify App",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/backend/src",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "port": 9229,
        "skipFiles": ["<node_internals>/**"],
        "restart": true,
        "timeout": 60000,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "protocol": "inspector",
        "outputCapture": "std"
      }
    ]
  }
  